{
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --sourcemap",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx ./migrate.ts",
    "test": "npm run test:client && npm run test:server",
    "test:client": "vitest run",
    "test:client:watch": "vitest",
    "test:server": "vitest run --config ./vitest.config.server.ts",
    "test:server:int": "vitest run --config ./vitest.config.server.ts",
    "test:server:watch": "vitest --config ./vitest.config.server.ts",
    "test:clear-cache": "vitest cache clean",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "audit:fix": "npm audit fix",
    "security:audit": "npm audit"
  },
